#include <stdio.h>
int main(void)
{
	int A, B;
	int C;
	scanf("%d %d", &A, &B);
	scanf("%d", &C);

	B += C;
	A += B / 60;
	if (B >= 60)
		if (A >= 24) 
		{
			A -= 24;
			B %= 60;
			printf("%d %d\n", A, B);
		}
		else 
		{
			B %= 60;
			printf("%d %d\n", A, B);
		}
	else
	{
		printf("%d %d\n", A, B);
	}
	return 0;
}
